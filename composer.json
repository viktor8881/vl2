{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for Zend Framework zend-mvc applications",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "mvc",
        "zf"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-component-installer": "^1.0 || ^0.3 || ^1.0.0-dev@dev",
        "zendframework/zend-mvc": "^3.0.1",
        "zfcampus/zf-development-mode": "^3.0",
        "zendframework/zend-mvc-form": "^1.0",
        "doctrine/doctrine-orm-module": "^1.1",
        "zendframework/zend-crypt": "^3.2",
        "zendframework/zend-session": "^2.7",
        "zendframework/zend-authentication": "^2.5",
        "zendframework/zend-mvc-plugin-flashmessenger": "^1.0",
        "zendframework/zend-mvc-i18n": "^1.0",
        "zendframework/zend-queue": "^2.0@RC",
        "zendframework/zend-mail": "^2.7",
        "zendframework/zend-i18n": "^2.7",
        "zendframework/zend-navigation": "^2.8",
        "zendframework/zend-captcha": "^2.7",
        "zendframework/zend-mvc-plugins": "^1.0",
        "zendframework/zend-test": "^3.1",
        "zendframework/zend-json": "^3.1",
        "panychek/moex": "^0.1.0",
        "zendframework/zend-log": "^2.10"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "Exchange\\": "module/Exchange/src/",
            "User\\": "module/User/src/",
            "Account\\": "module/Account/src/",
            "Course\\": "module/Course/src/",
            "Base\\": "module/Base/src/",
            "Task\\": "module/Task/src/",
            "Investments\\": "module/Investments/src/",
            "Cron\\": "module/Cron/src/",
            "Analysis\\": "module/Analysis/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/",
            "AnalysisTest\\": "module/Analysis/test/"
        }
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
        "test": "phpunit"
    }
}
